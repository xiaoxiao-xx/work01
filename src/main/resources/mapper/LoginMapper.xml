<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="wh.dao.LoginMapper">

	<select id="login" parameterType="wh.pojo.User" resultType="java.lang.String">
		select
			user_id
		from user_table
		where user_name=#{userName} and
			  password=#{password}
	</select>











</mapper>